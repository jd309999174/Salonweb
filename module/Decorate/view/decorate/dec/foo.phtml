<script>
$(document).ready(function(){
	var name=[[1,2,3],[4,5,6,6,4,5],[0,9,8,7,6,0],[3,2,1]];
	$("button").click(function(){
		$.post("foo",{
			name:name
		},
		function(data,status){
			alert("数据: \n" + data + "\n状态: " + status);
		});
	});
});
</script>

